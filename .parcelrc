{
  "extends": "@parcel/config-default",
  "transformers": {
    "*.{ts,tsx}": [
      "@parcel/transformer-typescript-tsc"
    ],
    "types:*.{ts,tsx}": [
      "@parcel/transformer-typescript-types"
    ],
    "*.html": [
      "@parcel/transformer-html"
    ],
    "types:*.html": [
      "..."
    ],
    "*.{png,jpg,jpeg}": [
      "@parcel/transformer-image"
    ]
  }
}